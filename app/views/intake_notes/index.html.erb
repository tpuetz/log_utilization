<h1>Intake Notes</h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th>ID</th>
      <th>Type</th>
      <th>Created at</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @intake_notes.each do |intake_note| %>
      <tr>
        <td><%= intake_note.id %></td>
        <td><%= link_to intake_note.note_type, intake_note_path(intake_note) %></td>
        <td><%= intake_note.created_at %></td>
        <td>
          <%= link_to 'Edit', edit_intake_note_path(intake_note), :class => 'btn btn-mini' %>
          <%= link_to 'Destroy', intake_note_path(intake_note), :method => :delete, :confirm => 'Are you sure?', :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New', new_intake_note_path, :class => 'btn btn-primary' %>
