<h1>/Case</h1>

<p>
  <b>Primary issue</b><br>
  <%= @case.primary_issue %>
</p>

<p>
  <b>Case manager</b><br>
  <%= @case.case_manager %>
</p>

<p>
  <b>Health insurance</b><br>
  <%= @case.health_insurance %>
</p>

<p>
  <b>Performance problem</b><br>
  <%= @case.performance_problem %>
</p>

<p>
  <b>Employee</b><br>
  <%= @case.employee_id %>
</p>

<div class="form-actions">
  <%= link_to 'Back', cases_path, :class => 'btn'  %>
  <%= link_to 'Edit', edit_case_path(@case), :class => 'btn' %>
  <%= link_to 'Delete', case_path(@case), :method => 'delete', :confirm => 'Are you sure?', :class => 'btn btn-danger' %>
</div>

<%= render @note %>